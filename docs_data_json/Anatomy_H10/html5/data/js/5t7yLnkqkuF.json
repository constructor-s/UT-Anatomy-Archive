n be picked \rup by the fenestrated capillary \rbeds of the primary plexus and \rdelivered to the anterior \rpituitary at the secondary plexus.","pngfb":false,"pr":{"l":"Lib","i":146}},"html5data":{"xPos":-1,"yPos":-1,"width":716,"height":434,"strokewidth":0}},"width":715,"height":433,"resume":false,"useHandCursor":true,"id":"5tL1NnQGOKp"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"6oP8yeyzw11_838970102","id":"01","linkId":"txt__default_6oP8yeyzw11","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":51,"bottom":24,"pngfb":false,"pr":{"l":"Lib","i":148}}}],"shapemaskId":"","xPos":650,"yPos":272,"tabIndex":4,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":30.5,"rotateYPos":14.5,"scaleX":100,"scaleY":100,"alpha":100,"depth":6,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":61,"bottom":29,"altText":"J20.05","pngfb":false,"pr":{"l":"Lib","i":128}},"html5data":{"xPos":-1,"yPos":-1,"width":62,"height":30,"strokewidth":0}},"width":61,"height":29,"resume":false,"useHandCursor":true,"id":"6oP8yeyzw11"},{"kind":"vectorshape","rotation":0,"accType":"button","cliptobounds":false,"defaultAction":"onrelease","textLib":[{"kind":"textdata","uniqueId":"60CnDYyAS6X_-2048901062","id":"01","linkId":"txt__default_60CnDYyAS6X","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":64,"bottom":22,"pngfb":false,"pr":{"l":"Lib","i":23}}}],"shapemaskId":"","xPos":624,"yPos":510,"tabIndex":6,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":44,"rotateYPos":12,"scaleX":100,"scaleY":100,"alpha":100,"depth":7,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":-2,"top":-2,"right":89,"bottom":25,"altText":"Next","pngfb":false,"pr":{"l":"Lib","i":22}},"html5data":{"xPos":