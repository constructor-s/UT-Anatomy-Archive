fferentiates to form the mesangium.","pngfb":false,"pr":{"l":"Lib","i":208}},"html5data":{"xPos":-1,"yPos":-1,"width":721,"height":223,"strokewidth":0}},"width":720,"height":222,"resume":false,"useHandCursor":true,"id":"6f9XeTIR99Z"},{"kind":"image","btnxpos":28,"btnypos":170,"zoomiconurl":"story_content/zoomIcon.png","zoomtype":"flashwindow","zoomslide":false,"zoomdata":{"hotlinkId":"","accState":0,"imagedata":{"assetId":25,"url":"","type":"normal","width":0,"height":0,"mobiledx":0,"mobiledy":0},"html5data":{"url":"6YrK5oq8Tur_DX972_DY380_CX972_CY380.png","xPos":0,"yPos":110,"width":972,"height":380,"strokewidth":0,"mask":"5A2D0X"}},"accType":"none","shapemaskId":"","xPos":5,"yPos":286,"tabIndex":4,"tabEnabled":true,"xOffset":-4,"yOffset":-4,"rotateXPos":253.5,"rotateYPos":99,"scaleX":100,"scaleY":100,"alpha":100,"rotation":0,"depth":6,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"imagedata":{"assetId":24,"url":"","type":"normal","width":0,"height":0,"mobiledx":0,"mobiledy":0},"html5data":{"url":"Shape6p56NsB3CMB.png","xPos":-4,"yPos":-4,"width":518,"height":210,"strokewidth":0,"mask":""}},"width":507,"height":198,"resume":false,"useHandCursor":true,"id":"6p56NsB3CMB"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"5hURYvZVpcl_-1733303358","id":"01","linkId":"txt__default_5hURYvZVpcl","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":199,"bottom":157,"pngfb":false,"pr":{"l":"Lib","i":211}}}],"shapemaskId":"","xPos":506,"yPos":284,"tabIndex":3,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":107,"rotateYPos":81,"scaleX":100,"scaleY":100,"alpha":100,"depth":7,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":214,"bottom":162,"altText":"The parietal layer of the glomerular capsule is separated from the visceral layer of the 