":136,"rotateYPos":14.5,"scaleX":100,"scaleY":100,"alpha":100,"depth":1,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":272,"bottom":29,"altText":"Rectal Varices","pngfb":false,"pr":{"l":"Lib","i":309}},"html5data":{"xPos":-1,"yPos":-1,"width":273,"height":30,"strokewidth":0}},"width":272,"height":29,"resume":false,"useHandCursor":true,"id":"6idzX0Fk4s7"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"6JDBaqLdBNl_467977240","id":"01","linkId":"txt__default_6JDBaqLdBNl","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":23,"bottom":76,"pngfb":false,"pr":{"l":"Lib","i":39}}}],"shapemaskId":"","xPos":8,"yPos":8,"tabIndex":3,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":16.5,"rotateYPos":37.5,"scaleX":100,"scaleY":100,"alpha":100,"depth":2,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":-1,"top":-1,"right":35,"bottom":77,"altText":"INFO","pngfb":false,"pr":{"l":"Lib","i":38}},"html5data":{"xPos":-1,"yPos":-1,"width":35,"height":77,"strokewidth":2}},"width":34,"height":76,"resume":false,"useHandCursor":true,"id":"6JDBaqLdBNl"}],"accType":"text","altText":"Group\r\n 1","shapemaskId":"","xPos":0,"yPos":48,"tabIndex":2,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":161,"rotateYPos":46,"scaleX":100,"scaleY":100,"alpha":100,"rotation":0,"depth":5,"scrolling":true,"shuffleLock":false,"width":322,"height":92,"resume":false,"useHandCursor":true,"id":"6dfNOwFWoft"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"6cPyxsrsTFm_-1738678800","id":"01","linkId":"txt__default_6cPyxsrsTFm","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":680,"bottom":81,"pngfb":false,"pr":{"l":"Lib","i":312}}}],"shapemaskId":"","xPos":38,"yPos":82,"tabIndex":5,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":345,"rotateYPos":43,"scaleX":100,"scaleY":100,"alpha":100,"depth":6,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":690,"bottom":86,"altText":"As described earlier, portal-systemic anastomoses connect veins of the hepatic portal system, which drains blood from the digestive tract to the liver, to systemic veins, which drain blood directly to the IVC or SVC. Recall that such a portal-systemic anastomosis occurs in the esophagus, and can lead to esophageal varices. A portal-systemic anastomosis also occurs in the submucosa of the rectum.","pngfb":false,"pr":{"l":"Lib","i":311}},"html5data":{"xPos":-1,"yPos":-1,"width":691,"height":87,"strokewidth":0}},"width":690,"height":86,"resume":false,"useHandCursor":true,"id":"6cPyxsrsTFm"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"6VtD7KEZaXn_799131645","id":"01","linkId":"txt__default_6VtD7KEZaXn","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":708,"bottom":249,"pngfb":false,"pr":{"l":"Lib","i":314}}}],"shapemaskId":"","xPos":0,"yPos":166,"tabIndex":6,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":360,"rotateYPos":127,"scaleX":100,"scaleY":100,"alpha":100,"depth":7,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":720,"bottom":254,"altText":"The superior rectal veins drain into the portal system. The middle and inferior rectal veins drain into the systemic system (the internal iliac veins, and from there into the IVC). These veins are connected by a portal-systemic anastomosis.\nWith hepatic portal hypertension, as may occur in cirrhosis of the liver secondary to alcoholic liver disease or viral hepatitis, veinous blood is diverted from the superior rectal vein to the tributaries of the middle and inferior rectal veins, located in the submucosa of the rectoanal junction. In response to the increased venous pressure, veins in the submucosa of the rectum dilate (rectal varices).\nThese are distinct from hemorrhoids, which are dilated vessels at the anorectal junction in the venous plexus associated with mucosa overlying the internal anal sphincter (internal hemorrhoids) and at the anus in the venous plexus associated with the skin overlying the external anal sphincter (external hemorrhoids). Hemorrhoids are not associated with portal hypertension, but, for instance, with a low fibre diet and the resultant straining associated with defecation.","pngfb":false,"pr":{"l":"Lib","i":313}},"html5data":{"xPos":-1,"yPos":-1,"width":721,"height":255,"strokewidth":0}},"width":720,"height":254,"resume":false,"useHandCursor":true,"id":"6VtD7KEZaXn"}],"startTime":-1,"elapsedTimeMode":"normal","useHandCursor":false,"resume":false,"kind":"slidelayer","isBaseLayer":true}]}
