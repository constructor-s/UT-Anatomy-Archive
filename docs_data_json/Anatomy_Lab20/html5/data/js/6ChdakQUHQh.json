 Posture & Stereotyped Mvts\nIt Coordinates Skilled Voluntary Movements","pngfb":false,"pr":{"l":"Lib","i":153}},"html5data":{"xPos":-1,"yPos":-1,"width":445,"height":452,"strokewidth":0}},"width":444,"height":451,"resume":false,"useHandCursor":true,"id":"5Zu2Z5769j7"},{"kind":"stategroup","objects":[{"kind":"expandinglabel","animationtype":"full","showclosebutton":false,"contentheight":0,"borderwidth":2,"arrowxpos":179,"arrowypos":-18,"objects":[{"kind":"scrollarea","contentwidth":348,"contentheight":54,"objects":[{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","textLib":[{"kind":"textdata","uniqueId":"6JbtUUycwkj_-817242214","id":"01","linkId":"txt__default_6JbtUUycwkj","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1,"vectortext":{"left":0,"top":0,"right":328,"bottom":36,"pngfb":false,"pr":{"l":"Lib","i":157}}}],"shapemaskId":"","xPos":0,"yPos":0,"tabIndex":8,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":174,"rotateYPos":18,"scaleX":100,"scaleY":100,"alpha":100,"depth":700,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":348,"bottom":36,"altText":"Remember: the names of pathways are your friends! They tell you the flow of information.","pngfb":false,"pr":{"l":"Lib","i":156}},"html5data":{"xPos":-1,"yPos":-1,"width":349,"height":37,"strokewidth":0}},"width":348,"height":36,"resume":false,"useHandCursor":true,"id":"6JbtUUycwkj"}],"shapemaskId":"","xPos":0,"yPos":0,"tabIndex":7,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":174,"rotateYPos":27,"scaleX":100,"scaleY":100,"alpha":100,"rotation":0,"depth":0,"scrolling":true,"shuffleLock":false,"width":348,"height":54,"resume":false,"useHandCursor":true,"id":"content_scroll_5qKdwMjU7k6"}],"textLib":[{"kind":"textdata","uniqueId":"5lg5AGJ990A_1676965099","id":"01","linkId":"txt__default_5lg5AGJ990A","type":"vectortext","xPos":0,"yPos":0,"width":0,"height":0,"shadowIndex":-1